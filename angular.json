{
  "version": 1,
  "projects": {
    "operators1": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/operators1",
      "sourceRoot": "apps/operators1/src",
      "prefix": "operators",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/operators1",
            "index": "apps/operators1/src/index.html",
            "main": "apps/operators1/src/main.ts",
            "polyfills": "apps/operators1/src/polyfills.ts",
            "tsConfig": "apps/operators1/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/operators1/src/favicon.ico",
              "apps/operators1/src/assets"
            ],
            "styles": ["apps/operators1/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/operators1/src/environments/environment.ts",
                  "with": "apps/operators1/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "operators1:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "operators1:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "operators1:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/operators1/tsconfig.app.json",
              "apps/operators1/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/operators1/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/operators1/jest.config.js",
            "tsConfig": "apps/operators1/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/operators1/src/test-setup.ts"
          }
        }
      }
    },
    "operators1-e2e": {
      "root": "apps/operators1-e2e",
      "sourceRoot": "apps/operators1-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/operators1-e2e/cypress.json",
            "tsConfig": "apps/operators1-e2e/tsconfig.e2e.json",
            "devServerTarget": "operators1:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "operators1:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/operators1-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/operators1-e2e/**"]
          }
        }
      }
    },
    "scan": {
      "projectType": "library",
      "root": "libs/scan",
      "sourceRoot": "libs/scan/src",
      "prefix": "operators",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/scan/tsconfig.lib.json",
              "libs/scan/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/scan/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/scan/jest.config.js",
            "tsConfig": "libs/scan/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/scan/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "tap": {
      "projectType": "library",
      "root": "libs/tap",
      "sourceRoot": "libs/tap/src",
      "prefix": "operators",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/tap/tsconfig.lib.json",
              "libs/tap/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/tap/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/tap/jest.config.js",
            "tsConfig": "libs/tap/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/tap/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "home": {
      "projectType": "library",
      "root": "libs/home",
      "sourceRoot": "libs/home/src",
      "prefix": "operators",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/home/tsconfig.lib.json",
              "libs/home/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/home/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/home/jest.config.js",
            "tsConfig": "libs/home/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/home/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "operators1"
}
